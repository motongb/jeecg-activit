<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.contract.mapper.LgContractBiddingItemMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  lg_contract_bidding_item 
		WHERE
			 bidding_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.contract.entity.LgContractBiddingItem">
		SELECT * 
		FROM  lg_contract_bidding_item
		WHERE
			 bidding_id = #{mainId} 	</select>
</mapper>
